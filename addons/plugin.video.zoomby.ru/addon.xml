<?xml version="1.0" encoding="UTF-8" standalone="yes"?>
<addon id="plugin.video.zoomby.ru"
       name="ZOOMBY.RU"
       version="1.0.2"
       provider-name="anonim">
  <requires>
    <import addon="xbmc.python" version="1.0"/>
  </requires>
  <extension point="xbmc.python.pluginsource"
             provides="video"
             library="default.py">
    <provides>video</provides>
  </extension>
  <extension point="xbmc.addon.metadata">
    <summary>Смотрите видео с сайта http://zoomby.ru/</summary>
    <description>Просто смотрите что хотите :)</description>
    <platform>all</platform>
